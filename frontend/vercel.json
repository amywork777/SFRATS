{
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "buildCommand": "CI=false GENERATE_SOURCEMAP=false TSC_COMPILE_ON_ERROR=true npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "installCommand": "npm install",
  "ignoreCommand": "echo 'Skipping checks'"
} 